{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "api": {
            "runtime": "python3.8",
            "includeFiles": [
              "requirements.txt"
            ],
            "buildCommand": "pip install -r requirements.txt"
          }
      }
    ],
    "routes": [
      { "src": "/(.*)", 
      "dest": "app.py" 
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }
  
  